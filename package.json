{
    "name": "phonydata",
    "version": "2.0.2",
    "description": "Generate test data, based on casual's ideas and faker's data.",
    "main": "lib/index.js",
    "scripts": {
        "build": "rm -rf lib && tsc",
        "test:ava": "ava",
        "test:lint": "eslint src test",
        "test": "npm run test:lint && npm run test:ava"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fidian/phonydata.git"
    },
    "keywords": [
        "phony",
        "fake",
        "data",
        "generate",
        "testing",
        "casual",
        "faker",
        "lorem",
        "random"
    ],
    "author": "Tyler Akins <fidian@rumkin.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fidian/phonydata/issues"
    },
    "homepage": "https://github.com/fidian/phonydata#readme",
    "devDependencies": {
        "@types/mersenne-twister": "^1.1.2",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "@typescript-eslint/parser": "^5.54.1",
        "ava": "5.2.0",
        "eslint": "^8.35.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "mersenne-twister": "^1.1.0"
    },
    "sideEffects": false,
    "ava": {
        "extensions": [ "ts" ],
        "files": [
            "test/**/*.ts"
        ],
        "require": [
            "ts-node/register/transpile-only"
        ]
    }
}
